  <!-- Solar Generators -->
  <ThingDef ParentName="BuildingBase">
    <defName>RPGrapheneSolarGenerator</defName>
    <label>Graphene Solar generator</label>
    <description>Produces electricity from sunlight. Does not work in the dark or under artificial light. More robust and produces slightly more power than a regular generator.</description>
    <thingClass>Building</thingClass>
    <!-- <minifiedDef>MinifiedThing</minifiedDef> -->
    <graphicData>
      <texPath>Things/Building/Power/RPGrapheneSolarCollector</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,4)</drawSize>
      <shadowData>
        <volume>(3.5,0.75,3.4)</volume>
        <offset>(0,0,0)</offset>
      </shadowData>
      <damageData>
        <rect>(0,0.6,4,2.8)</rect>
      </damageData>
    </graphicData>
    <castEdgeShadows>true</castEdgeShadows>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <drawerType>MapMeshAndRealTime</drawerType>
    <pathCost>70</pathCost>
    <fillPercent>0.5</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <!-- <Mass>10.0</Mass> -->
      <MaxHitPoints>750</MaxHitPoints>
      <WorkToBuild>3000</WorkToBuild>
      <Flammability>0.25</Flammability>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(4,4)</size>
    <costList>
      <Steel>20</Steel>
      <RimPlasGraphene>100</RimPlasGraphene>
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>RimPlas.CompPowerPlantSolarGraphene</compClass>
        <basePowerConsumption>-1</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable" />
    </comps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>Power</designationCategory>
    <designationHotKey>Misc5</designationHotKey>
    <constructEffect>ConstructMetal</constructEffect>
    <researchPrerequisites>
      <li>SolarPanels</li>
      <li>RimPlas_Graphene</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>7</constructionSkillPrerequisite>
  </ThingDef>
